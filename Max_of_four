#include <stdio.h>
   int max_of_four(int a,int b,int c,int d) {
       int max,m1,m2;
       if (a<b)
           m1=b;
       else
           m1=a;
       if (c<d)
           m2=d;
       else
           m2=c;
       if(m1<m2)
           max=m2;
       else
           max=m1;
       return max;
    }

int main() {
    int a, b, c, d;
    scanf("%d %d %d %d", &a, &b, &c, &d);
    int ans = max_of_four(a, b, c, d);
    printf("%d", ans);
    
    return 0;
}
